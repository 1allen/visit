{
    "name": "spatie/visit",
    "description": "Visit any URL and get its output in a beautiful way.",
    "keywords": ["visit", "url", "debugging", "curl"],
    "homepage": "https://github.com/spatie/visit",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/http": "^9.4",
        "illuminate/view": "^9.4",
        "nunomaduro/termwind": "^1.3",
        "soundasleep/html2text": "^2.0",
        "symfony/css-selector": "^6.0",
        "symfony/dom-crawler": "^6.0",
        "symfony/stopwatch": "^6.0"
    },
    "require-dev": {
        "laravel-zero/framework": "^9.0",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "build": "php visit app:build visit",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/visit"]
}
